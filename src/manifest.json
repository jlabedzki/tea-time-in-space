{
  "manifest_version": 3,
  "name": "Tea Time In Space",
  "description": "A new tab page that shows you interesting facts/images of space, the current location of the ISS, and astronauts currently in space.",
  "background": { "service_worker": "background.bundle.js" },
  "action": {
    "default_icon": "logo-32.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "logo-128.png"
  },
  "permissions": ["storage"],
  "web_accessible_resources": [
    {
      "resources": ["logo-128.png, logo-32.png"],
      "matches": []
    }
  ]
}
